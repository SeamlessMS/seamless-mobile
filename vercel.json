{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/submit-ticket",
      "dest": "/api/submit-ticket.js"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ]
} 